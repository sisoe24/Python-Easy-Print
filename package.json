{
  "name": "python-easy-print",
  "displayName": "Python Easy Print",
  "description": "Easy commands for some of Python most useful prints.",
  "version": "0.2.0",
  "publisher": "virgilsisoe",
  "icon": "images/python_icon_256px.png",
  "galleryBanner": {
    "color": "#347AB4",
    "theme": "dark"
  },
  "homepage": "https://github.com/sisoe24/Python-Easy-Print",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisoe24/Python-Easy-Print.git"
  },
  "bugs": {
    "email": "virgilsisoe@gmail.com",
    "url": "https://github.com/sisoe24/Python-Easy-Print/issues"
  },
  "keywords": [
    "python",
    "print",
    "debug",
    "quick",
    "easy",
    "helper",
    "comment",
    "uncomment",
    "delete",
    "repr",
    "pep"
  ],
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-easy-print.easyPrint",
        "title": "Python EasyPrint"
      },
      {
        "command": "python-easy-print.easyPrintDir",
        "title": "Python EasyPrint: dir"
      },
      {
        "command": "python-easy-print.easyPrintType",
        "title": "Python EasyPrint: type"
      },
      {
        "command": "python-easy-print.easyHelp",
        "title": "Python EasyPrint: help"
      },
      {
        "command": "python-easy-print.easyPrintRepr",
        "title": "Python EasyPrint: repr"
      },
      {
        "command": "python-easy-print.easyPrintPy2",
        "title": "Python EasyPrint: Easy print settings for Python2"
      },
      {
        "command": "python-easy-print.commentPrintLines",
        "title": "Python EasyPrint: Comment print lines"
      },
      {
        "command": "python-easy-print.uncommentPrintLines",
        "title": "Python EasyPrint: Uncomment print lines"
      },
      {
        "command": "python-easy-print.deletePrintLines",
        "title": "Python EasyPrint: Delete print lines"
      }
    ],
    "keybindings": [
      {
        "command": "python-easy-print.easyPrint",
        "key": "ctrl+shift+l p",
        "mac": "cmd+shift+l p",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.easyPrintDir",
        "key": "ctrl+shift+l d",
        "mac": "cmd+shift+l d",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.easyPrintType",
        "key": "ctrl+shift+l t",
        "mac": "cmd+shift+l t",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.easyPrintRepr",
        "key": "ctrl+shift+l r",
        "mac": "cmd+shift+l r",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.easyHelp",
        "key": "ctrl+shift+l h",
        "mac": "cmd+shift+l h",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.commentPrintLines",
        "key": "ctrl+shift+l c",
        "mac": "cmd+shift+l c",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.uncommentPrintLines",
        "key": "ctrl+shift+l u",
        "mac": "cmd+shift+l u",
        "when": "editorTextFocus"
      },
      {
        "command": "python-easy-print.deletePrintLines",
        "key": "ctrl+shift+l x",
        "mac": "cmd+shift+l x",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Python EasyPrint",
      "properties": {
        "pythonEasyPrint.customizeLogMessage": {
          "type": "string",
          "default": "",
          "description": "Custom log message for the print. Placeholder variables:\n%f - File name\n%t - Time\n%l - Line Number"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "choma": "^1.2.1",
    "eslint": "^8.2.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vscode-test": "^1.6.1"
  }
}